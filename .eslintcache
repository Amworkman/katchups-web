s = []\n\n  useEffect(() => {\n    storeDispatch(fetchFriends());    \n  }, [storeDispatch])\n\n  const handleChange = (e) => {\n    dispatch({name: e.target.name, value: e.target.value})\n  }\n\n  const searchFriendList = (friend) => {\n    if (friend.name.toLowerCase().includes(state.search.toLowerCase())){\n      parsedFriends.push(friend)\n    }\n  }\n\n  const renderFriends = (props) => {\n    friends.filter(searchFriendList)\n    return parsedFriends.map( friend => <\n      Friend\n        key={friend.id}\n        name={friend.name} \n        img={friend.profile_img_url}\n        location={friend.location}\n        email={friend.email}\n        handleSelectedFriend={props.handleSelectedFriend}\n    />)\n  }\n\n  \n\n  return (\n    \n    <div className='right-container'>\n      <input type=\"text\" id=\"friend-search\" name=\"search\" className=\"search\" value={state.search} onChange={handleChange} placeholder=\"Search Friends\" />\n      <div className=\"box-fade box-fade-top\"></div>\n      <div className='friend-container'>        \n        {renderFriends(props)}\n        {state.friendCard}    \n      </div>\n      <div className=\"box-fade box-fade-bottom\"></div>\n    </div>\n  );\n};\n\nexport default FriendsContainer;","/home/sheev/dev/katchups/katchups-web/src/Components/Main/Friends/Friend.js",["130"],"import React  from 'react';\nimport './FriendsContainer.scoped.css'\n\n\n\nconst Friend = (props) => {  \n  const handleSelectedFriend = (props) => {\n    props.handleSelectedFriend(props)\n  }\n\n  return (\n    <div>\n      <img  onClick={() => handleSelectedFriend(props)} className=\"list-img\" src={props.img} /><br />\n      <h2>{props.name}</h2> <br /> \n    </div>\n  );\n};\n\nexport default Friend;","/home/sheev/dev/katchups/katchups-web/src/Components/Main/Restaurants/Restaurant.js",["131"],"/home/sheev/dev/katchups/katchups-web/src/Components/Main/Friends/FriendCard.js",["132"],"/home/sheev/dev/katchups/katchups-web/src/Components/SignUp/SignU